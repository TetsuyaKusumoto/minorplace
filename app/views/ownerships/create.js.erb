<% if params[:type] == "Visit" %>
  $(".visit-<%= @place.id %>").replaceWith("<%= escape_javascript(render('places/unvisit' , place: @place)) %>");
<% else %>
  $(".want-<%= @place.id %>").replaceWith("<%= escape_javascript(render('places/unwant' , place: @place)) %>");
<% end %>

$('.btn-visit,.btn-want').removeAttr('disabled');
